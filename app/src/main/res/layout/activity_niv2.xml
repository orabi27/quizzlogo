<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.app.quiz.quizz.Niv2Activity">

    <ImageButton
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:id="@+id/imageButton12"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:src="@drawable/porto"
        android:scaleType="fitCenter"
        android:background="#ffffff" />

    <ImageButton
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:id="@+id/imageButton13"
        android:layout_alignBottom="@+id/imageButton12"
        android:layout_toEndOf="@+id/imageButton12"
        android:src="@drawable/atleticomadrid"
        android:scaleType="fitCenter"
        android:background="#ffffff" />

    <ImageButton
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:id="@+id/imageButton14"
        android:layout_alignBottom="@+id/imageButton13"
        android:layout_toEndOf="@+id/imageButton13"
        android:background="#ffffff"
        android:src="@drawable/roma"
        android:scaleType="fitCenter" />

    <ImageButton
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:id="@+id/imageButton15"
        android:layout_below="@+id/imageButton12"
        android:layout_toStartOf="@+id/imageButton13"
        android:src="@drawable/ajax"
        android:scaleType="fitCenter"
        android:background="#ffffff" />

    <ImageButton
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:id="@+id/imageButton16"
        android:layout_alignBottom="@+id/imageButton15"
        android:layout_toStartOf="@+id/imageButton14"
        android:background="#ffffff"
        android:src="@drawable/napoli"
        android:scaleType="fitCenter" />

    <ImageButton
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:id="@+id/imageButton17"
        android:layout_alignBottom="@+id/imageButton16"
        android:layout_toEndOf="@+id/imageButton16"
        android:src="@drawable/psv"
        android:scaleType="fitCenter"
        android:background="#ffffff" />

    <ImageButton
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:id="@+id/imageButton18"
        android:layout_below="@+id/imageButton15"
        android:layout_toStartOf="@+id/imageButton16"
        android:background="#ffffff"
        android:src="@drawable/palermo"
        android:scaleType="fitCenter" />

    <ImageButton
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:id="@+id/imageButton19"
        android:layout_alignBottom="@+id/imageButton18"
        android:layout_toStartOf="@+id/imageButton17"
        android:src="@drawable/liverpool"
        android:scaleType="fitCenter"
        android:background="#ffffff" />

    <ImageButton
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:id="@+id/imageButton20"
        android:background="#ffffff"
        android:src="@drawable/marseille"
        android:scaleType="fitCenter"
        android:layout_alignTop="@+id/imageButton19"
        android:layout_toEndOf="@+id/imageButton19" />

    <ImageButton
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:id="@+id/imageButton21"
        android:layout_alignParentBottom="true"
        android:layout_toStartOf="@+id/imageButton19"
        android:scaleType="fitCenter"
        android:src="@drawable/asmonaco"
        android:background="#ffffff" />

    <ImageButton
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:id="@+id/imageButton22"
        android:background="#ffffff"
        android:src="@drawable/wolfsburg"
        android:scaleType="fitCenter"
        android:layout_alignParentBottom="true"
        android:layout_toStartOf="@+id/imageButton23" />

    <ImageButton
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:id="@+id/imageButton23"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:src="@drawable/schalke"
        android:scaleType="fitCenter"
        android:background="#ffffff" />
</RelativeLayout>
